olve;\n        script.onerror = reject;\n        document.head.appendChild(script);\n      });\n    }\n    async function loadScriptsInOrder(scriptUrls) {\n      for (const url of scriptUrls) {\n        await loadScript(url);\n      }\n    }\n    onMounted(async () => {\n      await loadScriptsInOrder(['../vendor/jquery/jquery.min.js', '../vendor/chart.js/Chart.min.js', '../vendor/jquery-easing/jquery.easing.min.js', '../vendor/bootstrap/js/bootstrap.bundle.min.js', '../vendor/datatables/jquery.dataTables.min.js', '../js/sb-admin-2.min.js', '../vendor/datatables/dataTables.bootstrap4.min.js', '../js/demo/datatables-demo.js']);\n    });\n  }\n};","map":{"version":3,"names":["axios","onMounted","name","data","householdId","searchResults","validationStatus","methods","searchHousehold","response","post","encoderResponse","full_name","GRANTEE_NAME","validated","$router","push","params","entryId","ENTRY_ID","error","console","log","setup","loadScript","src","Promise","resolve","reject","script","document","createElement","onload","onerror","head","appendChild","loadScriptsInOrder","scriptUrls","url"],"sources":["C:\\laragon\\www\\frontend\\src\\components\\BUM.vue"],"sourcesContent":["\r\n    <template>\r\n      <div v-if=\"successMessage\" class=\"text-center text-success mt-3\">\r\n        {{ $store.state.successMessage }}\r\n      </div>\r\n        <div id=\"content-wrapper\" class=\"d-flex flex-column\">\r\n            <nav class=\"navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow text-center\">\r\n                    <!-- <h3 class=\"text-primary \">Benificiares Updating Module</h3> -->\r\n                    <div class=\"input-group d-flex justify-content-end align-items-end\">\r\n                      <input v-model=\"householdId\" type=\"text\" class=\"form-control bg-light border-0 small \" placeholder=\"Enter HouseHold-id\" style=\"margin-left: 70%\">\r\n                      <div class=\"input-group-append\">\r\n                          <button class=\"btn btn-primary\"  @click=\"searchHousehold\">\r\n                              <i class=\"fas fa-search fa-sm\"></i>\r\n                          </button>\r\n                      </div>\r\n                  </div>\r\n           </nav>  \r\n    </div>\r\n    <div v-if=\"validationStatus === 'alreadyValidated'\" class=\"text-center text-danger mt-3\">\r\n      Grantee Name is already validated.\r\n    </div>\r\n    <div v-if=\"searchResults.length > 0\">\r\n      <div class=\"card shadow mb-4\">\r\n        <div class=\"card-header py-3\">\r\n            <h6 class=\"m-0 font-weight-bold text-primary\">Search Results</h6>\r\n        </div>\r\n        <div class=\"card-body\">\r\n            <div class=\"table-responsive\">\r\n                <table class=\"table table-bordered\" id=\"dataTable\" width=\"100%\" cellspacing=\"0\">\r\n                    <thead>\r\n                        <tr class=\"text-center\">\r\n                            <th class=\"font-weight-bold\" >ACTION</th>\r\n                            <th class=\"font-weight-bold\">ENTRY_ID</th>\r\n                            <th class=\"font-weight-bold\">FULLNAME</th>\r\n                            <th class=\"font-weight-bold\">BIRTHDAY</th>\r\n                            <th class=\"font-weight-bold\">AGE</th>\r\n                            <th class=\"font-weight-bold\">SEX</th>\r\n                            <th class=\"font-weight-bold\">RELATIONSHIP</th>\r\n                            <th class=\"font-weight-bold\">CLIENT STATUS</th>\r\n                            <!-- <th class=\"font-weight-bold\">CHILD SELECTION</th> -->\r\n                            <th class=\"font-weight-bold\">CAPABLE GRANTEE</th>        \r\n                        </tr>\r\n                    </thead>\r\n                    \r\n                    <tbody>\r\n                        <tr v-for=\"info in searchResults\" :key=\"info.HH_ID\" class=\"text-center\">\r\n                            <td>\r\n                              <router-link :to=\"{ name: 'validate', params: { entryId: info.ENTRY_ID } }\"  class=\"nav-link\" >\r\n                                <button class=\"btn btn-primary\">\r\n                                    <i class=\"fas fa-edit\"></i> Update\r\n                                </button>\r\n                            </router-link>\r\n                            </td>\r\n                            <td>{{info.ENTRY_ID}}</td>\r\n                            <td>{{ `${info.LAST_NAME} ${info.FIRST_NAME} ${info.MIDDLE_NAME}`.trim() }}</td>\r\n                            <td>{{info.BIRTHDAY.split(' ')[0]}}</td>\r\n                            <td>{{info.AGE}}</td>\r\n                            <td>{{info.SEX}}</td>\r\n                            <td>{{info.RELATION_TO_HH_HEAD}}</td>\r\n                            <td>{{info.CLIENT_STATUS}}</td>\r\n                            <td>{{info.GRANTEE}}</td>\r\n                        </tr>                        \r\n                    </tbody>\r\n                </table> \r\n            </div>\r\n        </div>\r\n    </div>\r\n    </div>\r\n\r\n    <!-- <div v-else>\r\n      <p>No results found.</p>\r\n    </div> -->\r\n\r\n\r\n    </template>\r\n       \r\n    <script>\r\n    import axios from 'axios';\r\n    import { onMounted } from 'vue'\r\n    export default { \r\n      name: 'BUM',\r\n      data() {\r\n    return {\r\n      householdId: '',\r\n      searchResults: [],\r\n      validationStatus: '', \r\n    };\r\n  },\r\n\r\n//   created() {\r\n//   this.editDetails(/* pass the necessary details here */);\r\n// },\r\n  methods: {\r\n    //for searching function\r\n  //   searchHousehold() {\r\n  //     // Make an AJAX request to your server for searching\r\n  //     axios.post('/search', { householdId: this.householdId })\r\n  //       .then(response => {\r\n  //         // console.log('Raw Response:', response);\r\n  //         this.searchResults = response.data;\r\n  //       })\r\n  //       .catch(error => {\r\n  //         console.log(error);\r\n  //       });\r\n  //   },\r\n  // },\r\n  async searchHousehold() {\r\n      try {\r\n        const response = await axios.post('/search', { householdId: this.householdId });\r\n        this.searchResults = response.data;\r\n\r\n        // Check if the GRANTEE_NAME is already validated\r\n        const encoderResponse = await axios.post('/validateEncoder', { full_name: this.searchResults[0].GRANTEE_NAME });\r\n\r\n        if (encoderResponse.data.validated) {\r\n          // Set the validation status to display a message\r\n          this.validationStatus = 'alreadyValidated';\r\n        } else {\r\n          // If not validated, proceed to the second view for validation\r\n          this.$router.push({ name: 'validate', params: { entryId: this.searchResults[0].ENTRY_ID } });\r\n        }\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    },\r\n  },\r\n\r\n    //for loading js\r\n      setup() {\r\n        async function loadScript(src) {\r\n          return new Promise((resolve, reject) => {\r\n            const script = document.createElement('script');\r\n            script.src = src;\r\n            script.onload = resolve;\r\n            script.onerror = reject;\r\n            document.head.appendChild(script);\r\n          });\r\n        }\r\n    \r\n        async function loadScriptsInOrder(scriptUrls) {\r\n          for (const url of scriptUrls) {\r\n            await loadScript(url);\r\n          }\r\n        }\r\n    \r\n        onMounted(async () => {\r\n          await loadScriptsInOrder([\r\n    \r\n          '../vendor/jquery/jquery.min.js', \r\n      '../vendor/chart.js/Chart.min.js',   \r\n      '../vendor/jquery-easing/jquery.easing.min.js',   \r\n      '../vendor/bootstrap/js/bootstrap.bundle.min.js', \r\n      '../vendor/datatables/jquery.dataTables.min.js',    \r\n      '../js/sb-admin-2.min.js',  \r\n      '../vendor/datatables/dataTables.bootstrap4.min.js',  \r\