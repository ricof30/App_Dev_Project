\n        }\r\n    \r\n        async function loadScriptsInOrder(scriptUrls) {\r\n          for (const url of scriptUrls) {\r\n            await loadScript(url);\r\n          }\r\n        }\r\n    \r\n        onMounted(async () => {\r\n            // await loadScript('../vendor/chart.js/Chart.min.js'); \r\n            await loadScript('../vendor/jquery/jquery.min.js'); \r\n          await loadScriptsInOrder([\r\n    \r\n          '../vendor/jquery/jquery.min.js', \r\n        //   '../vendor/chart.js/Chart.min.js',   \r\n          '../vendor/jquery-easing/jquery.easing.min.js',   \r\n          '../vendor/bootstrap/js/bootstrap.bundle.min.js', \r\n          '../js/sb-admin-2.min.js',    \r\n        //   '../js/demo/chart-area-demo.js',\r\n        //   '../js/demo/chart-pie-demo.js'\r\n          ]);\r\n        });\r\n    },\r\n    };\r\n    </script>\r\n    \r\n    <style>\r\n    \r\n    @import '@/assets/assets/css/sb-admin-2.min.css';\r\n    @import '@/assets/assets/vendor/fontawesome-free/css/all.min.css';\r\n    @import '@/assets/assets/css/style.css';\r\n\r\n    </style>\r\n    \r\n    \r\n\r\n"],"mappings":"AAuEI,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,SAAQ,QAAS,KAAI;AAC9B,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACT,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,aAAa,EAAE;IAEjB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,eAAeA,CAAA,EAAG;MAChB;MACAP,KAAK,CAACQ,IAAI,CAAC,SAAS,EAAE;QAAEJ,WAAW,EAAE,IAAI,CAACA;MAAY,CAAC,EACpDK,IAAI,CAACC,QAAO,IAAK;QAChBC,OAAO,CAACC,GAAG,CAAC,eAAe,EAAEF,QAAQ,CAAC;QACtC,IAAI,CAACL,aAAY,GAAIK,QAAQ,CAACP,IAAI;MACpC,CAAC,EACAU,KAAK,CAACC,KAAI,IAAK;QACdH,OAAO,CAACC,GAAG,CAACE,KAAK,CAAC;MACpB,CAAC,CAAC;IACN;EACF,CAAC;EACGC,KAAKA,CAAA,EAAG;IACN,eAAeC,UAAUA,CAACC,GAAG,EAAE;MAC7B,OAAO,IAAIC,OAAO,CAAC,CAACC,OAAO,EAAEC,MAAM,KAAK;QACtC,MAAMC,MAAK,GAAIC,QAAQ,CAACC,aAAa,CAAC,QAAQ,CAAC;QAC/CF,MAAM,CAACJ,GAAE,GAAIA,GAAG;QAChBI,MAAM,CAACG,MAAK,GAAIL,OAAO;QACvBE,MAAM,CAACI,OAAM,GAAIL,MAAM;QACvBE,QAAQ,CAACI,IAAI,CAACC,WAAW,CAACN,MAAM,CAAC;MACnC,CAAC,CAAC;IACJ;IAEA,eAAeO,kBA