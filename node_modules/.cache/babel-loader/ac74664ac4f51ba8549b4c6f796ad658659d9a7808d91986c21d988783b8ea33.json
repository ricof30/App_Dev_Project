{"ast":null,"code":"import axios from 'axios';\nimport { onMounted } from 'vue';\nexport default {\n  name: 'BUM',\n  data() {\n    return {\n      householdId: '',\n      searchResults: [],\n      details: {} // Initialize details as an empty object\n    };\n  },\n\n  created() {\n    this.editDetails( /* pass the necessary details here */);\n  },\n  methods: {\n    //for searching function\n    searchHousehold() {\n      // Make an AJAX request to your server for searching\n      axios.post('/search', {\n        householdId: this.householdId\n      }).then(response => {\n        // console.log('Raw Response:', response);\n        this.searchResults = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    async editDetails(details) {\n      try {\n        // Make an AJAX request to CodeIgniter endpoint to get details\n        const response = await axios.get(`/getDetails/${details.ENTRY_ID}`);\n        // const response = await axios.post('/getDetails', { entryId: details.ENTRY_ID });\n\n        console.log(details.ENTRY_ID);\n        // Check if the response data is an array and not empty\n        this.details = response.data[0]; // Assuming your response is an array with a single object\n\n        // Check if details.ENTRY_ID has a value (optional)\n        if (this.details && this.details.ENTRY_ID) {\n          // Now you can access details.ENTRY_ID and other properties\n          console.log('Details from CodeIgniter:', JSON.parse(JSON.stringify(this.details)));\n          console.log('Details from CodeIgniter:', this.details.ENTRY_ID);\n        } else {\n          console.error('Invalid details received from CodeIgniter.');\n        }\n      } catch (error) {\n        console.error('Error fetching details:', error);\n      }\n    }\n  },\n  //for loading js\n  setup() {\n    async function loadScript(src) {\n      return new Promise((resolve, reject) => {\n        const script = document.createElement('script');\n        script.src = src;\n        script.onload = resolve;\n        script.onerror = reject;\n        document.head.appendChild(script);\n      });\n    }\n    async function loadScriptsInOrder(scriptUrls) {\n      for (const url of scriptUrls) {\n        await loadScript(url);\n      }\n    }\n    onMounted(async () => {\n      // await loadScript('../vendor/chart.js/Chart.min.js'); \n      await loadScript('../vendor/jquery/jquery.min.js');\n      await loadScriptsInOrder(['../vendor/jquery/jquery.min.js', '../vendor/chart.js/Chart.min.js', '../vendor/jquery-easing/jquery.easing.min.js', '../vendor/bootstrap/js/bootstrap.bundle.min.js', '../vendor/datatables/jquery.dataTables.min.js', '../js/sb-admin-2.min.js', '../vendor/datatables/dataTables.bootstrap4.min.js', '../js/demo/datatables-demo.js']);\n    });\n  }\n};","map":{"version":3,"names":["axios","onMounted","name","data","householdId","searchResults","details","created","editDetails","methods","searchHousehold","post","then","response","catch","error","console","log","get","ENTRY_ID","JSON","parse","stringify","setup","loadScript","src","Promise","resolve","reject","script","document","createElement","onload","onerror","head","appendChild","loadScriptsInOrder","scriptUrls","url"],"sources":["C:\\laragon\\www\\frontend\\src\\components\\BUM.vue"],"sourcesContent":["\r\n    <template>\r\n        <div id=\"content-wrapper\" class=\"d-flex flex-column\">\r\n            <nav class=\"navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow text-center\">\r\n                    <h3 class=\"text-primary \">Benificiares Updating Module</h3>\r\n                <div class=\"input-group search\">\r\n                  <input v-model=\"householdId\" type=\"text\" class=\"form-control  w-100\" style=\"max-width:500px; margin-left:350px;\" placeholder=\"Enter Household ID\" >\r\n                  <div class=\"input-group-append\">\r\n                    <button class=\"btn btn-primary\" @click=\"searchHousehold\">\r\n                      Search\r\n                    </button>\r\n                </div>\r\n              </div>\r\n           </nav>  \r\n    </div>\r\n\r\n    <div v-if=\"searchResults.length > 0\">\r\n      <div class=\"card shadow mb-4\">\r\n        <div class=\"card-header py-3\">\r\n            <h6 class=\"m-0 font-weight-bold text-primary\">Search Results</h6>\r\n        </div>\r\n        <div class=\"card-body\">\r\n            <div class=\"table-responsive\">\r\n                <table class=\"table table-bordered\" id=\"dataTable\" width=\"100%\" cellspacing=\"0\">\r\n                    <thead>\r\n                        <tr class=\"text-center\">\r\n                            <th class=\"font-weight-bold\" >ACTION</th>\r\n                            <th class=\"font-weight-bold\">ENTRY_ID</th>\r\n                            <th class=\"font-weight-bold\">FULLNAME</th>\r\n                            <th class=\"font-weight-bold\">BIRTHDAY</th>\r\n                            <th class=\"font-weight-bold\">AGE</th>\r\n                            <th class=\"font-weight-bold\">SEX</th>\r\n                            <th class=\"font-weight-bold\">RELATIONSHIP</th>\r\n                            <th class=\"font-weight-bold\">CLIENT STATUS</th>\r\n                            <!-- <th class=\"font-weight-bold\">CHILD SELECTION</th> -->\r\n                            <th class=\"font-weight-bold\">CAPABLE GRANTEE</th>        \r\n                        </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                        <tr v-for=\"info in searchResults\" :key=\"info.HH_ID\" class=\"text-center\">\r\n                            <td>\r\n                              <router-link :to=\"{ name: 'validate', params: { entryId: info.ENTRY_ID } }\" @click=\"editDetails(info)\" class=\"nav-link\" >\r\n                                <button class=\"btn btn-primary\">\r\n                                    <i class=\"fas fa-edit\"></i> Edit\r\n                                </button>\r\n                            </router-link>\r\n                            </td>\r\n                            <td>{{info.ENTRY_ID}}</td>\r\n                            <td>{{ `${info.LAST_NAME} ${info.FIRST_NAME} ${info.MIDDLE_NAME}`.trim() }}</td>\r\n                            <td>{{info.BIRTHDAY.split(' ')[0]}}</td>\r\n                            <td>{{info.AGE}}</td>\r\n                            <td>{{info.SEX}}</td>\r\n                            <td>{{info.RELATION_TO_HH_HEAD}}</td>\r\n                            <td>{{info.CLIENT_STATUS}}</td>\r\n                            <td>{{info.GRANTEE}}</td>\r\n                        </tr>                        \r\n                    </tbody>\r\n                </table> \r\n            </div>\r\n        </div>\r\n    </div>\r\n    </div>\r\n\r\n    <!-- <div v-else>\r\n      <p>No results found.</p>\r\n    </div> -->\r\n\r\n\r\n    </template>\r\n       \r\n    <script>\r\n    import axios from 'axios';\r\n    import { onMounted } from 'vue'\r\n    export default {\r\n      name: 'BUM',\r\n      data() {\r\n    return {\r\n      householdId: '',\r\n      searchResults: [],\r\n      details: {}, // Initialize details as an empty object\r\n\r\n    };\r\n  },\r\n\r\n  created() {\r\n  this.editDetails(/* pass the necessary details here */);\r\n},\r\n  methods: {\r\n    //for searching function\r\n    searchHousehold() {\r\n      // Make an AJAX request to your server for searching\r\n      axios.post('/search', { householdId: this.householdId })\r\n        .then(response => {\r\n          // console.log('Raw Response:', response);\r\n          this.searchResults = response.data;\r\n        })\r\n        .catch(error => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n    async editDetails(details) {\r\n    try {\r\n      // Make an AJAX request to CodeIgniter endpoint to get details\r\n      const response = await axios.get(`/getDetails/${details.ENTRY_ID}`);\r\n        // const response = await axios.post('/getDetails', { entryId: details.ENTRY_ID });\r\n      \r\n        console.log(details.ENTRY_ID)\r\n      // Check if the response data is an array and not empty\r\n      this.details = response.data[0]; // Assuming your response is an array with a single object\r\n\r\n      // Check if details.ENTRY_ID has a value (optional)\r\n      if (this.details && this.details.ENTRY_ID) {\r\n        // Now you can access details.ENTRY_ID and other properties\r\n      console.log('Details from CodeIgniter:', JSON.parse(JSON.stringify(this.details)));\r\n\r\n        console.log('Details from CodeIgniter:', this.details.ENTRY_ID);\r\n      } else {\r\n        console.error('Invalid details received from CodeIgniter.');\r\n      }\r\n    } catch (error) {\r\n      console.error('Error fetching details:', error);\r\n    }\r\n  },\r\n  },\r\n    //for loading js\r\n      setup() {\r\n        async function loadScript(src) {\r\n          return new Promise((resolve, reject) => {\r\n            const script = document.createElement('script');\r\n            script.src = src;\r\n            script.onload = resolve;\r\n            script.onerror = reject;\r\n            document.head.appendChild(script);\r\n          });\r\n        }\r\n    \r\n        async function loadScriptsInOrder(scriptUrls) {\r\n          for (const url of scriptUrls) {\r\n            await loadScript(url);\r\n          }\r\n        }\r\n    \r\n        onMounted(async () => {\r\n            // await loadScript('../vendor/chart.js/Chart.min.js'); \r\n            await loadScript('../vendor/jquery/jquery.min.js'); \r\n          await loadScriptsInOrder([\r\n    \r\n          '../vendor/jquery/jquery.min.js', \r\n      '../vendor/chart.js/Chart.min.js',   \r\n      '../vendor/jquery-easing/jquery.easing.min.js',   \r\n      '../vendor/bootstrap/js/bootstrap.bundle.min.js', \r\n      '../vendor/datatables/jquery.dataTables.min.js',    \r\n      '../js/sb-admin-2.min.js',  \r\n      '../vendor/datatables/dataTables.bootstrap4.min.js',  \r\n      '../js/demo/datatables-demo.js',  \r\n          ]);\r\n        });\r\n    },\r\n    };\r\n    </script>\r\n    \r\n    <style>\r\n    @import url('https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css');\r\n    @import url('https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i');\r\n    @import '@/assets/assets/css/sb-admin-2.min.css';\r\n    @import '@/assets/assets/vendor/fontawesome-free/css/all.min.css';\r\n    @import '@/assets/assets/css/style.css';\r\n    @import '@/assets/assets/vendor/datatables/dataTables.bootstrap4.min.css';\r\n\r\n    </style>\r\n    \r\n    \r\n\r\n"],"mappings":"AAuEI,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,SAAQ,QAAS,KAAI;AAC9B,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACT,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,aAAa,EAAE,EAAE;MACjBC,OAAO,EAAE,CAAC,CAAC,CAAE;IAEf,CAAC;EACH,CAAC;;EAEDC,OAAOA,CAAA,EAAG;IACV,IAAI,CAACC,WAAW,CAAsC,CAArC,sCAAsC;EACzD,CAAC;EACCC,OAAO,EAAE;IACP;IACAC,eAAeA,CAAA,EAAG;MAChB;MACAV,KAAK,CAACW,IAAI,CAAC,SAAS,EAAE;QAAEP,WAAW,EAAE,IAAI,CAACA;MAAY,CAAC,EACpDQ,IAAI,CAACC,QAAO,IAAK;QAChB;QACA,IAAI,CAACR,aAAY,GAAIQ,QAAQ,CAACV,IAAI;MACpC,CAAC,EACAW,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC,CAAC;IACN,CAAC;IAED,MAAMP,WAAWA,CAACF,OAAO,EAAE;MAC3B,IAAI;QACF;QACA,MAAMO,QAAO,GAAI,MAAMb,KAAK,CAACkB,GAAG,CAAE,eAAcZ,OAAO,CAACa,QAAS,EAAC,CAAC;QACjE;;QAEAH,OAAO,CAACC,GAAG,CAACX,OAAO,CAACa,QAAQ;QAC9B;QACA,IAAI,CAACb,OAAM,GAAIO,QAAQ,CAACV,IAAI,CAAC,CAAC,CAAC,EAAE;;QAEjC;QACA,IAAI,IAAI,CAACG,OAAM,IAAK,IAAI,CAACA,OAAO,CAACa,QAAQ,EAAE;UACzC;UACFH,OAAO,CAACC,GAAG,CAAC,2BAA2B,EAAEG,IAAI,CAACC,KAAK,CAACD,IAAI,CAACE,SAAS,CAAC,IAAI,CAAChB,OAAO,CAAC,CAAC,CAAC;UAEhFU,OAAO,CAACC,GAAG,CAAC,2BAA2B,EAAE,IAAI,CAACX,OAAO,CAACa,QAAQ,CAAC;QACjE,OAAO;UACLH,OAAO,CAACD,KAAK,CAAC,4CAA4C,CAAC;QAC7D;MACF,EAAE,OAAOA,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,yBAAyB,EAAEA,KAAK,CAAC