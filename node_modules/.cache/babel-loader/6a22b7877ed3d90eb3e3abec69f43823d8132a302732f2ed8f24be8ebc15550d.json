.details = {\r\n      ENTRY_ID: this.$route.params.entryId,\r\n      FULLNAME: state.fullname,\r\n      MUNICIPALITY: state.municipality,\r\n      HH_ID: state.hhId,\r\n      BRGY: state.brgy,\r\n      MEMBER_STATUS: '',\r\n      // Add more properties as needed\r\n    };\r\n  // Other component options...\r\n          // Accessing route parameters\r\n  \r\n      },\r\n      data() {\r\n    return {\r\n      details: {\r\n        ENTRY_ID: '',\r\n        FULLNAME: '',\r\n        MUNICIPALITY: '',\r\n        HH_ID: '',\r\n        BRGY: '',\r\n        MEMBER_STATUS: '',\r\n        // Add more properties as needed\r\n      },\r\n      // Other data properties...\r\n    };\r\n  },\r\n\r\n  methods:{\r\n    fetchDetails() {\r\n      // Make an AJAX request to CodeIgniter endpoint\r\n      axios.get(`/getDetails/${this.entryId}`)\r\n        .then(response => {\r\n          this.details = response.data;\r\n        })\r\n        .catch(error => {\r\n          console.error('Error fetching details:', error);\r\n        });\r\n    },\r\n  }\r\n    };\r\n  </script>\r\n  \r\n  <style scoped>\r\n    /* Add your custom styles here */\r\n    /* Use the \"scoped\" attribute to ensure styles only apply to this component */\r\n  </style>\r\n  "],"mappings":"AA6II,OAAOA,SAAQ,MAAO,WAAW;AACjC,OAAO,kCAAkC;AAEzC,eAAe;EACb;EACEC,KAAK,EAAE,CAAC,SAAS,CAAC;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;QACPC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,YAAY,EAAE,EAAE;QAChBC,KAAK,EAAE,EAAE;QACTC,IAAI,EAAE,EAAE;QACRC,aAAa,EAAE;QACf;MACF;IACF,CAAC;EACH,CAAC;;EACCC,IAAI,EAAC,WAAW;EAClBC,OAAOA,CAAA,EAAG;IACRX,SAAS,CAAC,gBAAgB,EAAE;MAAEY,UAAU,EAAE;IAAQ,CAAC,CAAC;IACpDZ,SAAS,CAAC,gBAAgB,EAAE;MAAEY,UAAU,EAAE;IAAQ,CAAC,CAAC;IAEpD,MAAMC,KAAI,GAAI,IAAI,CAACC,MAAM,CAACC,OAAO,CAACF,KAAK;IAC3C,IAAI,CAACV,OAAM,GAAI;MACbC,QAAQ,EAAE,IAAI,CAACU,MAAM,CAACE,MAAM,CAACC,OAAO;MACpCZ,QAAQ,EAAEQ,KAAK,CAACK,QAAQ;MACxBZ,YAAY,EAAEO,KAAK,CAACM,YAAY;MAChCZ,KAAK,EAAEM,KAAK,CAACO,IAAI;MACjBZ,IAAI,EAAEK,KAAK,CAACQ,IAAI;MAChB