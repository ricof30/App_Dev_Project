an access details.ENTRY_ID and other properties\r\n            console.log('Details from CodeIgniter:', JSON.parse(JSON.stringify(this.details)));\r\n          } else {\r\n            console.error('Invalid details received from CodeIgniter.');\r\n          }\r\n        } catch (error) {\r\n          console.error('Error fetching details:', error);\r\n        }\r\n      },\r\n    },\r\n      };\r\n  </script>\r\n  \r\n  <style>\r\n  \r\n  </style>"],"mappings":";AAgGI,OAAOA,KAAI,MAAO,OAAO;AAC3B,OAAOC,SAAQ,MAAO,WAAW;AAC7B,OAAO,kCAAkC;AAC7C,OAAOC,OAAM,MAAO,2BAA2B;AAC/C,OAAOC,MAAK,MAAO,0BAA0B;AACzC,eAAe;EACbC,UAAU,EAAC;IAACF,OAAO;IAACC;EAAM,CAAC;EACzBE,IAAI,EAAE,gBAAgB;EAEtBC,KAAK,EAAE,CAAC,SAAS,CAAC;EACxBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,CAAC,CAAC;MAAE;MACbC,OAAO,EAAE,EAAE;MACXC,aAAa,EAAE,EAAE;MACjBC,WAAW,EAAE;MACb;MACA;MACA;MACA;MACA;MACA;MACA;IACF,CAAC;EACH,CAAC;;EACDC,OAAOA,CAAA,EAAG;IACR,MAAMC,OAAM,GAAI,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,OAAO;IAC5CG,OAAO,CAACC,GAAG,CAAC,mBAAmB,EAAEJ,OAAO,CAAC;IACzC,IAAIA,OAAO,EAAE;MACX,IAAI,CAACK,WAAW,CAACL,OAAO,CAAC;IAC3B;EACF,CAAC;EACDM,KAAK,EAAE;IACPX,OAAO,EAAE;MACPY,OAAOA,CAAA,EAAG;QACR;QACA,IAAI,CAACC,SAAQ,GAAK,GAAE,IAAI,CAACb,OAAO,CAACc,UAAU,IAAI,IAAI,CAACd,OAAO,CAACe,WAAW,IAAI,IAAI,CAACf,OAAO,CAACgB,SAAU,EAAC,CAACC,IAAI,CAAC,CAAC;MAC5G,CAAC;MACDC,IAAI,EAAE,IAAI,CAAE;IACd;EACF,CAAC;;EAEGC,OAAOA,CAAA,EAAG;IACJ1B,SAAS,CAAC,gBAAgB,EAAE;MAAE2B,UAAU,EAAE;IAAQ,CAAC,CAAC;IACpD3B,SAAS,CAAC,gBAAgB,EAAE;MAAE2B,UAAU,EAAE;IAAQ,CAAC,CAAC;EACtD,CAAC;EACLC,OAAO,EAAE;IACP,MAAMC,MAAMA,CAAA,EAAE;MAEV,IAAI;QACF,MAAMC,GAAE,GAAI,MAAM/B,KAAK,CAACgC,IAAI,CAAE,UAAS,IAAI,CAACxB,OAAO,CAACyB,QAAS,EAAC,EAAE;UAC5DxB,OAAO,EAAE,IAAI,CAACA,OAAO;UACrBC,aAAa,EAAE,IAAI,CAACA,aAAa;UACjCC,WAAW,EAAE,IAAI,CAACA;QACpB,CAAC,CAAC;QACF,IAAIoB,GAAG,CAACG,MAAK,KAAM,GAAG,EAAE;UACtB;UACA,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,WAAW,CAAC;QAEhC,OAAO;UACL;UACApB,OAAO,CAACqB,KAAK,CAAC,+BAA+B,EAAEN,GAAG,CAAC;QACrD;MACJ,EAAE,OAAOM,KAAK,EAAE;QACdrB,OAAO,CAACqB,KAAK,CAAC,uBAAuB,