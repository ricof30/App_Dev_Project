ort axios from 'axios'\r\n  export default {\r\n    data() {\r\n      return {\r\n        email: '',\r\n      };\r\n    },\r\n    methods: {\r\n        async submitForm() {\r\n      try {\r\n        const response = await axios.post(`/forgot/${this.email}`, { email: this.email });\r\n     \r\n        const responseData = JSON.parse(response);\r\n\r\n// Access the id property directly from the parsed object\r\nconst id = responseData.id;\r\n\r\n// Check if id is not undefined before using it\r\nif (id !== undefined) {\r\n  localStorage.setItem('id', id);\r\n  console.log(id);\r\n} else {\r\n  console.error('ID is undefined in the response.');\r\n}\r\n\r\n       \r\n        // Handle the response from the server here\r\n      } catch (error) {\r\n        console.error(error.response);\r\n      }\r\n    },\r\n    },\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* Add custom styles or override Bootstrap styles here */\r\n  </style>\r\n  "],"mappings":"AAsBE,OAAOA,KAAI,MAAO,OAAM;AACxB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,UAAUA,CAAA,EAAG;MACrB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAML,KAAK,CAACM,IAAI,CAAE,WAAU,IAAI,CAACJ,KAAM,EAAC,EAAE;UAAEA,KAAK,EAAE,IAAI,CAACA;QAAM,CAAC,CAAC;QAEjF,MAAMK,YAAW,GAAIC,IAAI,CAACC,KAAK,CAACJ,QAAQ,CAAC;;QAEjD;QACA,MAAMK,EAAC,GAAIH,YAAY,CAACG,EAAE;;QAE1B;QACA,IAAIA,EAAC,KAAMC,SAAS,EAAE;UACpBC,YAAY,CAACC,OAAO,CAAC,IAAI,EAAEH,EAAE,CAAC;UAC9BI,OAAO,CAACC,GAAG,CAACL,EAAE,CAAC;QACjB,OAAO;UACLI,OAAO,CAACE,KAAK,CAAC,kCAAkC,CAAC;QACnD;;QAGQ;MACF,EAAE,OAAOA,KAAK,EAAE;QACdF,OAAO,CAACE,KAAK,CAACA,KAAK,CAACX,QAAQ,CAAC;MAC/B;IACF;EACA;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}