                  <span v-if=\"!isValidFullName\" class=\"text-danger\">Please enter a valid name</span>\r\n                                            </div>\r\n                                        </div>\r\n                                        <!-- Last Name Input -->\r\n                                        <div class=\"form-group row\">\r\n                                            <div>\r\n                                                <input v-model=\"email\" type=\"text\" class=\"form-control form-control-user\" placeholder=\"Email\">\r\n                                                <span v-if=\"!isValidEmail\" class=\"text-danger\">Please enter a valid Email</span>\r\n                                            </div>\r\n                                        </div>\r\n                                        <!-- Password Input -->\r\n                                        <div class=\"form-group row\">\r\n                                            <div>\r\n                                                <input v-model=\"password\" type=\"password\" class=\"form-control form-control-user\" placeholder=\"Password\">\r\n                                                <span v-if=\"!isValidPassword\" class=\"text-danger\">Please enter a valid password</span>\r\n                                            </div>\r\n                                        </div>\r\n                                        <!-- Repeat Password Input -->\r\n                                        <div class=\"form-group row\">\r\n                                            <div>\r\n                                                <input v-model=\"repeatPassword\" type=\"password\" class=\"form-control form-control-user\" placeholder=\"Repeat Password\">\r\n                                                <span v-if=\"!isValidRepeatPassword\" class=\"text-danger\">Passwords do not match</span>\r\n                                            </div>\r\n                      