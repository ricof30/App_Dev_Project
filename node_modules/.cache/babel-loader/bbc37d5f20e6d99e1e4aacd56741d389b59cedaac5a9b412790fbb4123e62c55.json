{"ast":null,"code":"import axios from 'axios';\nimport { onMounted } from 'vue';\nexport default {\n  name: 'BUM',\n  data() {\n    return {\n      householdId: '',\n      searchResults: []\n    };\n  },\n  methods: {\n    //for searching function\n    searchHousehold() {\n      // Make an AJAX request to your server for searching\n      axios.post('/search', {\n        householdId: this.householdId\n      }).then(response => {\n        // console.log('Raw Response:', response);\n        this.searchResults = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    //for inserting function\n    editRow(entryId, lastName, firstName, middleName, municipality, hh_id, brgy, hh_status) {\n      // Step 1: Fetch details from beneficiaries table\n      axios.get(`/getDetails/${entryId}`).then(response => {\n        const details = response.data;\n        if (details && Object.keys(details).length > 0) {\n          // Add other details to the object\n          details.name = `${lastName} ${firstName} ${middleName}`.trim();\n          details.municipality = municipality;\n          details.hh_id = hh_id;\n          details.brgy = brgy;\n          details.hh_status = hh_status;\n          // Add other fields\n          console.log(details);\n\n          //  if (details.name) {\n          //   console.log('Name has a value:', details.name);\n          // } else {\n          //   console.log('Name is empty or undefined.');\n          // }\n          // Step 2: Insert details into encoder table\n          this.insertDetailsIntoEncoder(details);\n          // Step 3: Display Update Information Form with fetched details\n          // You need to implement the logic to show the form and pass the details to it\n        } else {\n          console.error('Details are empty or undefined.');\n        }\n        // Step 3: Display Update Information Form with fetched details\n        // You need to implement the logic to show the form and pass the details to it\n      }).catch(error => {\n        console.error('Error fetching details:', error);\n      });\n    },\n    insertDetailsIntoEncoder(details) {\n      // Step 4: Send a request to insert details into encoder table\n      axios.post('/insertDetails', details).then(response => {\n        console.log('Details inserted into encoder:', response.data);\n      }).catch(error => {\n        console.error('Error inserting details into encoder:', error);\n      });\n    }\n  },\n  //for loading js\n  setup() {\n    async function loadScript(src) {\n      return new Promise((resolve, reject) => {\n        const script = document.createElement('script');\n        script.src = src;\n        script.onload = resolve;\n        script.onerror = reject;\n        document.head.appendChild(script);\n      });\n    }\n    async function loadScriptsInOrder(scriptUrls) {\n      for (const url of scriptUrls) {\n        await loadScript(url);\n      }\n    }\n    onMounted(async () => {\n      // await loadScript('../vendor/chart.js/Chart.min.js'); \n      await loadScript('../vendor/jquery/jquery.min.js');\n      await loadScriptsInOrder(['../vendor/jquery/jquery.min.js', '../vendor/chart.js/Chart.min.js', '../vendor/jquery-easing/jquery.easing.min.js', '../vendor/bootstrap/js/bootstrap.bundle.min.js', '../vendor/datatables/jquery.dataTables.min.js', '../js/sb-admin-2.min.js', '../vendor/datatables/dataTables.bootstrap4.min.js', '../js/demo/datatables-demo.js']);\n    });\n  }\n};","map":{"version":3,"names":["axios","onMounted","name","data","householdId","searchResults","methods","searchHousehold","post","then","response","catch","error","console","log","editRow","entryId","lastName","firstName","middleName","municipality","hh_id","brgy","hh_status","get","details","Object","keys","length","trim","insertDetailsIntoEncoder","setup","loadScript","src","Promise","resolve","reject","script","document","createElement","onload","onerror","head","appendChild","loadScriptsInOrder","scriptUrls","url"],"sources":["C:\\laragon\\www\\frontend\\src\\components\\BUM.vue"],"sourcesContent":["\r\n    <template>\r\n        <div id=\"content-wrapper\" class=\"d-flex flex-column\">\r\n            <nav class=\"navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow text-center\">\r\n                    <h3 class=\"text-primary \">Benificiares Updating Module</h3>\r\n                <div class=\"input-group search\">\r\n                  <input v-model=\"householdId\" type=\"text\" class=\"form-control  w-100\" style=\"max-width:500px; margin-left:350px;\" placeholder=\"Enter Household ID\" >\r\n                  <div class=\"input-group-append\">\r\n                    <button class=\"btn btn-primary\" @click=\"searchHousehold\">\r\n                      Search\r\n                    </button>\r\n                </div>\r\n              </div>\r\n           </nav>  \r\n    </div>\r\n\r\n    <div v-if=\"searchResults.length > 0\">\r\n      <div class=\"card shadow mb-4\">\r\n        <div class=\"card-header py-3\">\r\n            <h6 class=\"m-0 font-weight-bold text-primary\">Search Results</h6>\r\n        </div>\r\n        <div class=\"card-body\">\r\n            <div class=\"table-responsive\">\r\n                <table class=\"table table-bordered\" id=\"dataTable\" width=\"100%\" cellspacing=\"0\">\r\n                    <thead>\r\n                        <tr class=\"text-center\">\r\n                            <th class=\"font-weight-bold\" >ACTION</th>\r\n                            <th class=\"font-weight-bold\">ENTRY_ID</th>\r\n                            <th class=\"font-weight-bold\">FULLNAME</th>\r\n                            <th class=\"font-weight-bold\">BIRTHDAY</th>\r\n                            <th class=\"font-weight-bold\">AGE</th>\r\n                            <th class=\"font-weight-bold\">SEX</th>\r\n                            <th class=\"font-weight-bold\">RELATIONSHIP</th>\r\n                            <th class=\"font-weight-bold\">CLIENT STATUS</th>\r\n                            <!-- <th class=\"font-weight-bold\">CHILD SELECTION</th> -->\r\n                            <th class=\"font-weight-bold\">CAPABLE GRANTEE</th>        \r\n                        </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                        <tr v-for=\"info in searchResults\" :key=\"info.HH_ID\" class=\"text-center\">\r\n                            <td>\r\n                              <router-link to=\"validate\">\r\n                                <button @click=\"editRow(info.ENTRY_ID, info.LAST_NAME, info.FIRST_NAME, info.MIDDLE_NAME, info.MUNICIPALITY, info.HH_ID, info.BRGY, info.HH_STATUS)\" class=\"btn btn-primary\">\r\n                                  <i class=\"fas fa-edit\"></i> Edit\r\n                              </button>\r\n                              </router-link>\r\n                            </td>\r\n                            <td>{{info.ENTRY_ID}}</td>\r\n                            <td>{{ `${info.LAST_NAME} ${info.FIRST_NAME} ${info.MIDDLE_NAME}`.trim() }}</td>\r\n                            <td>{{info.BIRTHDAY.split(' ')[0]}}</td>\r\n                            <td>{{info.AGE}}</td>\r\n                            <td>{{info.SEX}}</td>\r\n                            <td>{{info.RELATION_TO_HH_HEAD}}</td>\r\n                            <td>{{info.CLIENT_STATUS}}</td>\r\n                            <td>{{info.GRANTEE}}</td>\r\n                        </tr>                        \r\n                    </tbody>\r\n                </table>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    </div>\r\n\r\n    <!-- <div v-else>\r\n      <p>No results found.</p>\r\n    </div> -->\r\n\r\n\r\n    </template>\r\n       \r\n    <script>\r\n    import axios from 'axios';\r\n    import { onMounted } from 'vue'\r\n    export default {\r\n      name: 'BUM',\r\n      data() {\r\n    return {\r\n      householdId: '',\r\n      searchResults: [],\r\n    \r\n    };\r\n  },\r\n  methods: {\r\n    //for searching function\r\n    searchHousehold() {\r\n      // Make an AJAX request to your server for searching\r\n      axios.post('/search', { householdId: this.householdId })\r\n        .then(response => {\r\n          // console.log('Raw Response:', response);\r\n          this.searchResults = response.data;\r\n        })\r\n        .catch(error => {\r\n          console.log(error);\r\n        });\r\n    },\r\n\r\n      //for inserting function\r\n      editRow(entryId, lastName, firstName, middleName, municipality, hh_id, brgy, hh_status) {\r\n    // Step 1: Fetch details from beneficiaries table\r\n    axios.get(`/getDetails/${entryId}`)\r\n        .then(response => {\r\n            const details = response.data;\r\n\r\n            if (details && Object.keys(details).length > 0) {\r\n                // Add other details to the object\r\n                details.name = `${lastName} ${firstName} ${middleName}`.trim();\r\n                details.municipality = municipality;\r\n                details.hh_id = hh_id;\r\n                details.brgy = brgy;\r\n                details.hh_status = hh_status;\r\n                // Add other fields\r\n               console.log(details);\r\n               \r\n              //  if (details.name) {\r\n              //   console.log('Name has a value:', details.name);\r\n              // } else {\r\n              //   console.log('Name is empty or undefined.');\r\n              // }\r\n                // Step 2: Insert details into encoder table\r\n                this.insertDetailsIntoEncoder(details);\r\n                // Step 3: Display Update Information Form with fetched details\r\n                // You need to implement the logic to show the form and pass the details to it\r\n            } else {\r\n                console.error('Details are empty or undefined.');\r\n            }\r\n            // Step 3: Display Update Information Form with fetched details\r\n            // You need to implement the logic to show the form and pass the details to it\r\n        })\r\n        .catch(error => {\r\n            console.error('Error fetching details:', error);\r\n        });\r\n},\r\n\r\n    insertDetailsIntoEncoder(details) {\r\n      // Step 4: Send a request to insert details into encoder table\r\n      axios.post('/insertDetails', details)\r\n        .then(response => {\r\n          console.log('Details inserted into encoder:', response.data);\r\n        })\r\n        .catch(error => {\r\n          console.error('Error inserting details into encoder:', error);\r\n        });\r\n    },\r\n\r\n  },\r\n\r\n\r\n    //for loading js\r\n      setup() {\r\n        async function loadScript(src) {\r\n          return new Promise((resolve, reject) => {\r\n            const script = document.createElement('script');\r\n            script.src = src;\r\n            script.onload = resolve;\r\n            script.onerror = reject;\r\n            document.head.appendChild(script);\r\n          });\r\n        }\r\n    \r\n        async function loadScriptsInOrder(scriptUrls) {\r\n          for (const url of scriptUrls) {\r\n            await loadScript(url);\r\n          }\r\n        }\r\n    \r\n        onMounted(async () => {\r\n            // await loadScript('../vendor/chart.js/Chart.min.js'); \r\n            await loadScript('../vendor/jquery/jquery.min.js'); \r\n          await loadScriptsInOrder([\r\n    \r\n          '../vendor/jquery/jquery.min.js', \r\n      '../vendor/chart.js/Chart.min.js',   \r\n      '../vendor/jquery-easing/jquery.easing.min.js',   \r\n      '../vendor/bootstrap/js/bootstrap.bundle.min.js', \r\n      '../vendor/datatables/jquery.dataTables.min.js',    \r\n      '../js/sb-admin-2.min.js',  \r\n      '../vendor/datatables/dataTables.bootstrap4.min.js',  \r\n      '../js/demo/datatables-demo.js',  \r\n          ]);\r\n        });\r\n    },\r\n    };\r\n    </script>\r\n    \r\n    <style>\r\n    @import url('https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css');\r\n    @import url('https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i');\r\n    @import '@/assets/assets/css/sb-admin-2.min.css';\r\n    @import '@/assets/assets/vendor/fontawesome-free/css/all.min.css';\r\n    @import '@/assets/assets/css/style.css';\r\n    @import '@/assets/assets/vendor/datatables/dataTables.bootstrap4.min.css';\r\n\r\n    </style>\r\n    \r\n    \r\n\r\n"],"mappings":"AAuEI,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,SAAQ,QAAS,KAAI;AAC9B,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACT,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,aAAa,EAAE;IAEjB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP;IACAC,eAAeA,CAAA,EAAG;MAChB;MACAP,KAAK,CAACQ,IAAI,CAAC,SAAS,EAAE;QAAEJ,WAAW,EAAE,IAAI,CAACA;MAAY,CAAC,EACpDK,IAAI,CAACC,QAAO,IAAK;QAChB;QACA,IAAI,CAACL,aAAY,GAAIK,QAAQ,CAACP,IAAI;MACpC,CAAC,EACAQ,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC,CAAC;IACN,CAAC;IAEC;IACAG,OAAOA,CAACC,OAAO,EAAEC,QAAQ,EAAEC,SAAS,EAAEC,UAAU,EAAEC,YAAY,EAAEC,KAAK,EAAEC,IAAI,EAAEC,SAAS,EAAE;MAC1F;MACAvB,KAAK,CAACwB,GAAG,CAAE,eAAcR,OAAQ,EAAC,EAC7BP,IAAI,CAACC,QAAO,IAAK;QACd,MAAMe,OAAM,GAAIf,QAAQ,CAACP,IAAI;QAE7B,IAAIsB,OAAM,IAAKC,MAAM,CAACC,IAAI,CAACF,OAAO,CAAC,CAACG,MAAK,GAAI,CAAC,EAAE;UAC5C;UACAH,OAAO,CAACvB,IAAG,GAAK,GAAEe,QAAQ,IAAIC,SAAS,IAAIC,UAAW,EAAC,CAACU,IAAI,CAAC,CAAC;UAC9DJ,OAAO,CAACL,YAAW,GAAIA,YAAY;UACnCK,OAAO,CAACJ,KAAI,GAAIA,KAAK;UACrBI,OAAO,CAACH,IAAG,GAAIA,IAAI;UACnBG,OAAO,CAACF,SAAQ,GAAIA,SAAS;UAC7B;UACDV,OAAO,CAACC,GAAG,CAACW,OAAO,CAAC;;UAErB;UACA;UACA;UACA;UACA;UACE;UACA,IAAI,CAACK,wBAAwB,CAACL,OAAO,CAAC;UACtC;UACA;QACJ,OAAO;UACHZ,OAAO,CAACD,KAAK,CAAC,iCAAiC,CAAC;QACpD;QACA;QACA;MACJ,CAAC,EACAD,KAAK,CAACC,KAAI,IAAK;QACZC,OAAO,CAACD,KAAK,CAAC,yBAAyB,EAAEA,KAAK,CAAC;MACnD,CAAC,CAAC;IACV,CAAC;IAEGkB,wBAAwBA,CAACL,OAAO,EAAE;MAChC;MACAzB,KAAK,CAACQ,IAAI,CAAC,gBAAgB,EAAEiB,OAAO,EACjChB,IAAI,CAACC,QAAO,IAAK;QAChBG,OAAO,CAACC,GAAG,CAAC,gCAAgC,EAAEJ,Q